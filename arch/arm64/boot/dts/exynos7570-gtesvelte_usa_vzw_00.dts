/dts-v1/;
#include "exynos7570-rmem.dtsi"
#include "exynos7570-gtesvelte_common.dtsi"
#include "modem-ss305ap-pdata.dtsi"
#include "exynos7570-gtesvelte_battery.dtsi"
#include "exynos7570-gtesvelte_usa_vzw_gpio_00.dtsi"

/ {
	model = "Samsung GTESVELTE USA VZW board based on Exynos7570";
        model_info-chip = <7570>;
        model_info-platform = "android";
        model_info-subtype = "samsung";
        model_info-hw_rev = <0>;
        model_info-hw_rev_end = <0>;
	compatible = "samsung,exynos7570", "samsung,exynos4210";


	pinctrl@139F0000 {
		acc_int: acc_int {
			samsung,pins = "gpa1-0";
			samsung,pin-function = <0xF>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		grip_int: grip_int {
			samsung,pins = "gpa0-2";
			samsung,pin-function = <0xF>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};

		grip_wifi_int: grip_wifi_int {
			samsung,pins = "gpa0-0";
			samsung,pin-function = <0xF>;
			samsung,pin-pud = <0>;
			samsung,pin-drv = <0>;
		};
	};

	pinctrl@139B0000 {
		grip_wifi_i2c: grip_wifi_i2c {
			samsung,pins = "gpf0-2", "gpf0-3";
			samsung,pin-function = <2>;
			samsung,pin-pud = <3>;
		};
	};

	i2c@138D0000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";

		k2hh-i2c@1D {
			compatible = "k2hh-i2c";
			reg = <0x1D>;
			pinctrl-names = "default";
			pinctrl-0 = <&acc_int>;
			interrupt-parent = <&gpa1>;
			interrupts = <0 0 0>;
			k2hh,irq_gpio = <&gpa1 0 0>;
			k2hh,vio-supply = <&ldo2_reg>;
			k2hh,axis_map_x = <1>;
			k2hh,axis_map_y = <0>;
			k2hh,axis_map_z = <2>;
			k2hh,negate_x = <1>;
			k2hh,negate_y = <0>;
			k2hh,negate_z = <1>;
			k2hh,poll_interval = <100>;
			k2hh,min_interval = <2>;
		};
	};

	i2c@13830000 {
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";
		#address-cells = <1>;
		#size-cells = <0>;
		pinctrl-names = "default";		
		
		sx9306-i2c@28 {
			compatible = "sx9306-i2c";
			reg = <0x28>;
			pinctrl-names = "default";
			pinctrl-0 = <&grip_int>;
			interrupt-parent = <&gpa0>;
			interrupts = <2 0 0>;
			sx9306-i2c,nirq-gpio = <&gpa0 2 0>;
			#sx9306-i2c,ctrl0 = <0x14>; /* Scanperiod : 60 */
			sx9306-i2c,ctrl2 = <0x77>; /* Gain: 8, Freq : 167, Resolution : Finest */
			sx9306-i2c,ctrl3 = <0x03>; /* Rawfilter: High */
			sx9306-i2c,ctrl4 = <0x40>; /* AVGTHRESH */
			sx9306-i2c,ctrl5 = <0x36>; /* AVGNEG: 110 AVGPOS: 110 */
			sx9306-i2c,normal-thd = <5>; /* normal touch threshold */
			sx9306-i2c,ch-main = <2>; /* channel number */
			sx9306-i2c,ch-sub1 = <0xff>; /*oxff: Not be used sub channel */
			sx9306-i2c,ta-thd = <0x0f>; /* TA threhold : 300 */
		};
	};

	hsi2c@138C0000 {
		status = "okay";
		pinctrl-names = "default";
		pinctrl-0 = <&grip_wifi_i2c>;	

		sx9306_wifi-i2c@28{
			compatible = "sx9306_wifi-i2c";
			reg = <0x28>;
			pinctrl-names = "default";
			pinctrl-0 = <&grip_wifi_int>;
			interrupt-parent = <&gpa0>;
			interrupts = <0 0 0>;
			sx9306_wifi-i2c,nirq-gpio = <&gpa0 0 0>;
			#sx9306_wifi-i2c,ctrl0 = <0x04>; /* Scanperiod : 30 */
			sx9306_wifi-i2c,ctrl2 = <0x77>; /* Gain:8, Freq : 167, Resolution : Finest */
			sx9306_wifi-i2c,ctrl3 = <0x03>; /* Rawfilter: High */
			#sx9306_wifi-i2c,ctrl5 = <0x0f>; /* AVGNEG:001 AVGPOS : 111 */
			sx9306_wifi-i2c,normal-thd = <4>; /* normal touch threshold */
			sx9306_wifi-i2c,ch-main = <2>; /* channel number */
			sx9306_wifi-i2c,ch-sub1 = <0xff>; /*oxff: Not be used sub channel */
			sx9306_wifi-i2c,ta-thd = <0x0f>; /* TA threhold : 300 */
		};
	};

	pinctrl@139F0000 {
		dwmmc2_tflash_en: dwmmc2_tflash_en {
			   samsung,pins = "gpa0-3";
			   samsung,pin-function = <0x1>;
			   samsung,pin-val = <1>;
			   samsung,pin-pud = <0>;
			   samsung,pin-drv = <4>;
		};
	};

	dwmmc2@13560000 {
		pinctrl-names = "default",
			"fast-slew-rate-1x",
			"fast-slew-rate-2x",
			"fast-slew-rate-3x",
			"fast-slew-rate-4x";
		pinctrl-0 = <&sd2_clk &sd2_cmd &sd2_bus1 &sd2_bus4
			     &dwmmc2_cd_ext_irq &dwmmc2_tflash_en>;
		pinctrl-1 = <&sd2_clk_fast_slew_rate_1x>;
		pinctrl-2 = <&sd2_clk_fast_slew_rate_2x>;
		pinctrl-3 = <&sd2_clk_fast_slew_rate_3x>;
		pinctrl-4 = <&sd2_clk_fast_slew_rate_4x>;
	};

	sd_fixed_vcc: fixedregulator@1 {
		      compatible = "regulator-fixed";
		      regulator-name = "vmmc";
		      gpio = <&gpa0 3 1>;
		      regulator-min-microvolt = <2800000>;
		      regulator-max-microvolt = <2800000>;
		      regulator-initial-mode = <0>;
		      enable-active-high;
		      regulator-boot-on;
	};
	
	/* modem_interface */
	mif_pdata {
		reg = <0 0x10000 0x0>;

		pinctrl-names = "default";
		pinctrl-0 = <&cp_gpio_tabes_vzw>;
	};

	/* USIM DETECTION FOR CP */
	usim_det {
		pinctrl-names = "default";
		pinctrl-0 = <&sim1_gpio_on5>;

		mif,num_of_usim_det = <1>;
		mif,usim-det0-gpio = <&gpa1 4 0>;
		
	};
};
